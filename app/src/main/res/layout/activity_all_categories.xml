<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    xmlns:circular="http://schemas.android.com/apk/res-auto"
    android:layout_height="match_parent">
    <jlabs.soswagger.StarView.StarActivity
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        android:gravity="center_horizontal">
        <RelativeLayout
            android:layout_width="match_parent"
            android:id="@+id/actionbar"
            android:layout_height="56dp">
            <ImageView
                android:layout_width="wrap_content"
                android:src="@drawable/bar"
                android:paddingTop="15dp"
                android:paddingLeft="-10dp"
                android:id="@+id/nav"
                android:paddingBottom="15dp"
                android:layout_height="wrap_content" />
            <ImageView
                android:layout_width="match_parent"
                android:src="@drawable/letter"
                android:padding="13dp"
                android:layout_height="wrap_content" />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_alignParentRight="true"
                android:src="@drawable/drop"
                android:id="@+id/drop"
                android:padding="15dp"
                android:paddingRight="-7dp"
                android:layout_height="wrap_content" />

        </RelativeLayout>
        <RelativeLayout
            android:layout_below="@+id/actionbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/userbar"
          android:gravity="center_vertical"
           >
            <FrameLayout
                android:layout_width="wrap_content"

                android:layout_height="wrap_content">
                <TextView
                    android:layout_width="64dp"
                    android:layout_gravity="center"
                    android:layout_height="64dp"
                    android:background="@drawable/gol2"/>
                <TextView
                    android:layout_width="60dp"
                    android:layout_gravity="center"
                    android:layout_height="60dp"
                    android:layout_margin="20dp"
                    android:background="@drawable/gol1"/>

                <jlabs.soswagger.Rounded.RoundedImageView
                    android:layout_width="56dp"
                    android:layout_margin="20dp"
                    android:layout_gravity="center"
                    android:id="@+id/photo"
                    android:contentDescription="@string/app_name"
                    android:src="@drawable/view"
                    android:layout_height="56dp" />
            </FrameLayout>
            <RelativeLayout
                android:layout_width="wrap_content"

              android:layout_centerInParent="true"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_width="wrap_content"
                    android:background="@drawable/name"
                    android:text="Kashyap Sharma"
                    android:textSize="12sp"
                    android:textColor="#455F97"
                    android:paddingTop="3dp"
                    android:layout_centerHorizontal="true"
                    android:paddingBottom="3dp"
                    android:paddingLeft="10dp"
                    android:id="@+id/user"
                    android:paddingRight="10dp"
                    android:layout_height="wrap_content" />
                <TextView
                    android:layout_width="wrap_content"
                    android:id="@+id/occu"
                    android:text="Application Developer"
                    android:textSize="10sp"
                    android:layout_below="@+id/user"
                    android:textColor="#fff"
                    android:layout_marginTop="6dp"
                    android:paddingLeft="10dp"

                    android:paddingRight="10dp"
                    android:layout_height="wrap_content" />
            </RelativeLayout>
            <TextView

                android:layout_width="64dp"
                android:layout_height="64dp"
                android:textColor="#FDF300"
                android:layout_alignParentRight="true"
                android:text="42"
                android:layout_marginRight="15dp"
                android:textSize="35sp"
                android:textStyle="bold"
                android:layout_centerVertical="true"
                android:gravity="center"
                android:background="@drawable/golbutton"
               />

        </RelativeLayout>
        <TextView
            android:layout_below="@+id/userbar"
            android:id="@+id/all_cat"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:textSize="18sp"
            android:textStyle="bold"
            android:text="ALL CATEGORIES"/>

        <RelativeLayout
            android:layout_below="@+id/all_cat"
            android:layout_width="match_parent"
            android:id="@+id/bt1_pan"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"

            >
           <ImageButton
               android:layout_width="74dp"
               android:layout_height="74dp"
               android:src="@drawable/shoppingcart"
              android:scaleType="centerInside"
               android:padding="10dp"
               android:layout_margin="20dp"
               android:background="@drawable/cat_but"/>
            <ImageButton
                android:layout_width="74dp"
                android:layout_centerInParent="true"
                android:src="@drawable/phone"
                android:scaleType="centerInside"
                android:padding="10dp"
                android:background="@drawable/cat_but"
                android:layout_marginTop="20dp"
                android:layout_height="74dp">

            </ImageButton>
            <ImageButton
                android:layout_alignParentRight="true"
                android:layout_width="74dp"
                android:layout_margin="20dp"
                android:src="@drawable/building"
                android:scaleType="centerInside"
                android:padding="10dp"
                android:background="@drawable/cat_but"
                android:layout_centerInParent="true"
                android:layout_height="74dp">

            </ImageButton>

        </RelativeLayout>
        <RelativeLayout
            android:layout_below="@+id/bt1_pan"
            android:id="@+id/bt1_pan1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"


            >
            <ImageButton
                android:layout_width="74dp"
                android:layout_height="74dp"
                android:layout_marginLeft="20dp"
                android:src="@drawable/diamond"
                android:scaleType="centerInside"
                android:padding="10dp"
                android:layout_marginRight="20dp"

                android:background="@drawable/cat_but"/>
            <ImageButton
                android:layout_width="74dp"
                android:layout_centerInParent="true"
                android:background="@drawable/cat_but"
                android:src="@drawable/burger"
                android:scaleType="centerInside"
                android:padding="10dp"
                android:layout_centerVertical="true"
                android:layout_height="74dp">

            </ImageButton>
            <ImageButton
                android:layout_alignParentRight="true"
                android:layout_width="74dp"
                android:layout_marginLeft="20dp"
                android:src="@drawable/drink"
                android:scaleType="centerInside"
                android:padding="10dp"
                android:layout_marginRight="20dp"

                android:background="@drawable/cat_but"

                android:layout_height="74dp">

            </ImageButton>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/bt1_pan1"
            android:gravity="center_horizontal"

            >
            <ImageButton
                android:layout_width="74dp"
                android:src="@drawable/video_cam"
                android:scaleType="centerInside"
                android:padding="10dp"
                android:layout_height="74dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginRight="20dp"
                android:background="@drawable/cat_but"/>
            <ImageButton
                android:layout_width="74dp"
                android:layout_marginTop="40dp"
                android:src="@drawable/flight"
                android:scaleType="centerInside"
                android:padding="10dp"
                android:layout_centerInParent="true"
                android:background="@drawable/cat_but"
                android:layout_height="74dp">

            </ImageButton>
            <ImageButton
                android:layout_alignParentRight="true"
                android:layout_width="74dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                android:src="@drawable/cab"
                android:scaleType="centerInside"
                android:padding="10dp"
                android:layout_marginRight="20dp"
                android:background="@drawable/cat_but"

                android:layout_height="74dp">

            </ImageButton>

        </RelativeLayout>





<Button
    android:layout_width="wrap_content"
    android:layout_height="30dp"
    android:text="VIEW MORE"
    android:background="@drawable/view_more"
    android:textColor="#000000"
    android:paddingLeft="40dp"
    android:layout_marginBottom="20dp"
    android:textSize="14sp"
    android:paddingRight="40dp"
    android:layout_centerHorizontal="true"
    android:layout_alignParentBottom="true"/>



    </RelativeLayout>





</FrameLayout>
