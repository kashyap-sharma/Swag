<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    xmlns:circular="http://schemas.android.com/apk/res-auto"
    android:layout_height="match_parent">
    <jlabs.soswagger.StarView.StarActivity
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:gravity="center_horizontal">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="56dp">
        <ImageView
            android:layout_width="wrap_content"
            android:src="@drawable/bar"
            android:paddingTop="15dp"
            android:paddingLeft="-10dp"
            android:id="@+id/nav"
            android:paddingBottom="15dp"
            android:layout_height="wrap_content" />
        <ImageView
            android:layout_width="match_parent"
            android:src="@drawable/letter"
            android:padding="13dp"
            android:layout_height="wrap_content" />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_alignParentRight="true"
            android:src="@drawable/drop"
            android:id="@+id/drop"
            android:padding="15dp"
            android:paddingRight="-7dp"
            android:layout_height="wrap_content" />

    </RelativeLayout>

        <include layout="@layout/content_activity_login"
           />

        <TextView
            android:layout_width="wrap_content"
            android:background="@drawable/name"
            android:text="Kashyap Sharma"
            android:textSize="12sp"
            android:textColor="#455F97"
            android:paddingTop="3dp"
            android:paddingBottom="3dp"
            android:paddingLeft="10dp"
            android:id="@+id/user"
            android:paddingRight="10dp"
            android:layout_marginTop="-10dp"
            android:layout_height="wrap_content" />
        <TextView
            android:layout_width="wrap_content"
            android:id="@+id/occu"
            android:text="Application Developer"
            android:textSize="10sp"
            android:textColor="#fff"
           android:layout_marginTop="6dp"

            android:layout_height="wrap_content" />
        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
           >
            <jlabs.soswagger.customCompo.CircularSlider
                android:id="@+id/circular"
                android:layout_width="240dp"
                android:layout_height="240dp"
                android:padding="10dp"
                 android:layout_margin="5dp"
                circular:angle="3.0"
                android:layout_gravity="center"
                circular:border_color="#3A589B"
                circular:border_thickness="5dp"
                circular:thumb_image="@drawable/fb_gol"
                circular:thumb_size="24dp"/>
            <jlabs.soswagger.customCompo.CircularSlider
                android:id="@+id/circular1"
                android:layout_width="210dp"
                android:layout_height="210dp"
                android:padding="10dp"
                android:layout_gravity="center"
                circular:angle="10.14"
                circular:border_color="#2AAAE0"
                circular:border_thickness="5dp"
                circular:thumb_image="@drawable/twitter_gol"
                circular:thumb_size="24dp"/>
            <jlabs.soswagger.customCompo.CircularSlider
                android:id="@+id/circular2"
                android:layout_width="180dp"
                android:layout_height="180dp"
                android:padding="10dp"
                android:layout_gravity="center"
                circular:angle="5.14"
                circular:border_color="#D1C2A3"
                circular:border_thickness="5dp"
                circular:thumb_image="@drawable/instagram"
                circular:thumb_size="24dp"/>
            <TextView
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:textColor="#FDF300"
                android:text="42"
                android:textSize="60sp"
                android:textStyle="bold"
                android:gravity="center"
                android:background="@drawable/golbutton"
                android:layout_gravity="center"/>
        </FrameLayout>



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:layout_gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="30dp"
                android:id="@+id/fb_login"


                android:scaleType="centerCrop"
                android:src="@drawable/fb"
                android:layout_height="30dp" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1140"
                android:layout_marginLeft="5dp"
                android:layout_gravity="center_vertical"
                android:textColor="#fff"/>
            <ImageView
                android:layout_width="30dp"

                android:layout_marginLeft="15dp"
                android:id="@+id/gplus_login"


                android:scaleType="centerCrop"
                android:src="@drawable/twitter"
                android:layout_height="30dp" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1989"
                android:layout_marginLeft="5dp"
                android:layout_gravity="center_vertical"
                android:textColor="#fff"/>
            <ImageView
                android:layout_width="30dp"
                android:id="@+id/insta_login"
                android:layout_marginLeft="15dp"
                android:scaleType="centerCrop"
                android:src="@drawable/instagram"
                android:layout_height="30dp" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="510"
                android:layout_marginLeft="5dp"
                android:layout_gravity="center_vertical"
                android:textColor="#fff"/>


        </LinearLayout>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="30dp"
            android:src="@drawable/swagmeter"/>

    </LinearLayout>





</FrameLayout>
